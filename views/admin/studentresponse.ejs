<%-include("header.ejs")%>
<div class="container p-5">
    <div class="row ">
        <h4> <%-stu_res.stu_name%></h4>
        <font> Response of <%-stu_res.quiz_name%></font><br>
         <font> Marks Obtain :<%-stu_res.obtained_marks%></font>
<div class="col-md-12 rounded shadow p-4 mt-5">
    <button class="btn btn-success" disabled>Selected</button>
     <button class="btn btn-primary" disabled>Correct Option</button>
<%for(let i=0;i<stu_res.que.length;i++){%>
<h5 class="mt-3"><%-i+1%> . <%-stu_res.que[i]%></h5>

<div class="clear-both p-2 ms-3 <%-(stu_res.cor_option[i]=="Option-1")?"bg-primary":""%> <%-(stu_res.cor_option[i]=="Option-1"&&stu_res.stu_ch[i]==stu_res.cor_option[i].toLowerCase())?"bg-success text-light":""%> rounded mb-2 <%-(stu_res.stu_ch[i]=="option-1")?"bg-success text-light":""%>"  style="width: 300px;">
<%-"1. "+stu_res.opt_1[i]%>
</div>
<div class="clear-both p-2 ms-3 <%-(stu_res.cor_option[i]=="Option-2")?"bg-primary":""%> <%-(stu_res.cor_option[i]=="Option-2"&&stu_res.stu_ch[i]==stu_res.cor_option[i].toLowerCase())?"bg-success text-light":""%> rounded mb-2 <%-(stu_res.stu_ch[i]=="option-2")?"bg-success text-light":""%>"  style="width: 300px;">
<%-"2. "+stu_res.opt_2[i]%>
</div>
<div class="clear-both p-2 ms-3 <%-(stu_res.cor_option[i]=="Option-3")?"bg-primary":""%> <%-(stu_res.cor_option[i]=="Option-3"&&stu_res.stu_ch[i]==stu_res.cor_option[i].toLowerCase())?"bg-success text-light":""%> rounded mb-2 <%-(stu_res.stu_ch[i]=="option-3")?"bg-success text-light":""%>"  style="width: 300px;">
<%-"3. "+stu_res.opt_3[i]%>
</div>
<div class="clear-both p-2 ms-3 <%-(stu_res.cor_option[i]=="Option-4")?"bg-primary text-light":""%> <%-(stu_res.cor_option[i]=="Option-4"&&stu_res.stu_ch[i]==stu_res.cor_option[i].toLowerCase())?"bg-success text-light":""%> rounded mb-2 <%-(stu_res.stu_ch[i]=="option-4")?"bg-success text-light":""%>"  style="width: 300px;">
<%-"4. "+stu_res.opt_4[i]%>
</div>

<%}%>
</div>

    </div>
</div>




<%-include("footer.ejs")%>